# Auto White Paper - Environment Variables
# Copy this file to .env and fill in your values

# Claude API (Anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Genspark API (for literature research)
GENSPARK_API_KEY=your_genspark_api_key_here

# GitHub Token (optional, for private repos)
GITHUB_TOKEN=your_github_token_here

# Output Settings
AWP_OUTPUT_DIR=./output
AWP_CACHE_DIR=./.awp/cache

# Logging
AWP_LOG_LEVEL=info

# LLM Settings
AWP_DEFAULT_MODEL=claude-sonnet-4-20250514
AWP_MAX_TOKENS=4096
AWP_TEMPERATURE=0.7
